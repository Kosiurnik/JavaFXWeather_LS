<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="320.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="sda.lukaszs.weatherfx.Controller.WeatherFXController">
    <AnchorPane prefHeight="312.0" prefWidth="640.0" VBox.vgrow="ALWAYS">
        <TextField fx:id="fxTextFieldCity" layoutX="115.0" layoutY="9.0" prefHeight="31.0" prefWidth="241.0"
                   text="Bydgoszcz"/>
        <Button fx:id="fxButtonSearch" layoutX="356.0" layoutY="9.0" mnemonicParsing="false" onAction="#setCity"
                text="Szukaj"/>
        <Label layoutX="10.0" layoutY="14.0" prefHeight="17.0" prefWidth="149.0" text="Podaj Miasto :"/>
        <Label layoutX="14.0" layoutY="90.0" prefHeight="21.0" prefWidth="139.0" text="Kraj"/>
        <Label layoutX="14.0" layoutY="72.0" prefHeight="21.0" prefWidth="139.0" text="Miasto"/>
        <Label layoutX="14.0" layoutY="110.0" prefHeight="21.0" prefWidth="139.0" text="Region"/>
        <Label layoutX="14.0" layoutY="131.0" prefHeight="21.0" prefWidth="139.0" text="Położenie"/>
        <Label fx:id="fxLocationCity" layoutX="165.0" layoutY="74.0" prefHeight="21.0" prefWidth="252.0"
               text="fxMiasto"/>
        <Label fx:id="fxLocationCountry" layoutX="165.0" layoutY="92.0" prefHeight="21.0" prefWidth="252.0"
               text="fxKraj"/>
        <Label fx:id="fxLocationRegion" layoutX="165.0" layoutY="110.0" prefHeight="21.0" prefWidth="252.0"
               text="fxRegion"/>
        <Label fx:id="fxLocationCoordinates" layoutX="165.0" layoutY="131.0" prefHeight="21.0" prefWidth="252.0"
               text="fxPołożenie"/>

        <ImageView fx:id="fxImageConditionIcon" fitHeight="150.0" fitWidth="200.0" layoutX="440.0"
                   pickOnBounds="true" preserveRatio="true"/>
        <Label fx:id="fxConditionDayTime" layoutX="468.0" layoutY="294.0" prefHeight="21.0" prefWidth="172.0"
               text="fxDzienLubNoc" textAlignment="RIGHT"/>
        <Label fx:id="fxConditionDescription" layoutX="438.0" layoutY="150.0" prefHeight="21.0" prefWidth="200.0"
               text="fxPogoda" textAlignment="CENTER"/>
        <Label fx:id="fxConditionTemperature" layoutX="465.0" layoutY="209.0" prefHeight="84.0" prefWidth="172.0"
               text="TEMP" textAlignment="CENTER">
            <font>
                <Font size="48.0"/>
            </font>
        </Label>
        <Label layoutX="14.0" layoutY="43.0" text="LOKALIZACJA">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <Label layoutX="14.0" layoutY="210.0" prefHeight="21.0" prefWidth="172.0" text="Prędkość wiatru (km/h)"/>
        <Label layoutX="14.0" layoutY="182.0" text="POGODA">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
        <Label layoutX="14.0" layoutY="231.0" prefHeight="21.0" prefWidth="172.0" text="Kierunek wiatru"/>
        <Label layoutX="14.0" layoutY="252.0" prefHeight="21.0" prefWidth="172.0" text="Ciśnienie atmosferyczne"/>
        <Label layoutX="14.0" layoutY="273.0" prefHeight="21.0" prefWidth="172.0" text="Wilgotność powietrza"/>
        <Label layoutX="14.0" layoutY="294.0" prefHeight="21.0" prefWidth="172.0" text="Zachmurzenie"/>
        <Label fx:id="fxCurrentWindSpeed" layoutX="186.0" layoutY="210.0" prefHeight="21.0" prefWidth="252.0"
               text="fxPrędkość wiatru"/>
        <Label fx:id="fxCurrentWindDirection" layoutX="186.0" layoutY="231.0" prefHeight="21.0" prefWidth="252.0"
               text="fxKierunekWiatru"/>
        <Label fx:id="fxCurrentPressure" layoutX="186.0" layoutY="252.0" prefHeight="21.0" prefWidth="252.0"
               text="fxCisnienie"/>
        <Label fx:id="fxCurrentHumidity" layoutX="186.0" layoutY="273.0" prefHeight="21.0" prefWidth="252.0"
               text="fxWilgotnosc"/>
        <Label fx:id="fxCurrentCloud" layoutX="186.0" layoutY="294.0" prefHeight="21.0" prefWidth="252.0"
               text="fxZachmurzenie"/>
        <Label layoutX="378.0" layoutY="182.0" text="TEMPERATURA">
            <font>
                <Font name="System Bold" size="18.0"/>
            </font>
        </Label>
    </AnchorPane>
</VBox>
